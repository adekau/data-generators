{
    "name": "semble-ts",
    "version": "1.0.0-rc.6",
    "description": "Mock unit test data generation",
    "main": "dist/cjs/index.js",
    "module": "dist/esm5/index.js",
    "es2015": "./dist/esm/index.js",
    "esnext": "./dist/esm5/index.js",
    "types": "index.d.ts",
    "typesVersions": {
        ">=4.1": {
            "*": [
                "dist/types/*"
            ]
        }
    },
    "sideEffects": false,
    "scripts": {
        "test": "karma start",
        "prettier": "prettier --write .",
        "clean:docs": "shx rm -rf ./docs",
        "build:clean": "shx rm -rf ./dist",
        "build:all": "npm run build:clean && echo \"compiling...\" && tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.types.json && echo done",
        "build": "npm run prettier && npm run build:all && node ./scripts/alias",
        "build:docs": "npm run clean:docs && typedoc",
        "fix-typedoc-pages": "shx cp ./node_modules/typedoc-default-themes/bin/default/partials/type.hbs ./node_modules/typedoc-plugin-pages/dist/theme/v2/partials/type.hbs"
    },
    "files": [
        "dist/cjs/**/*",
        "dist/esm/**/!(*.tsbuildinfo)",
        "dist/esm5/**/*",
        "dist/types/**/*",
        "library",
        "interface",
        "transformer",
        "compiler",
        "tsconfig.json"
    ],
    "exports": {
        ".": {
            "node": "./dist/cjs/index.js",
            "default": "./dist/esm5/index.js"
        },
        "./library": {
            "node": "./dist/cjs/library/index.js",
            "default": "./dist/esm5/library/index.js"
        },
        "./transformer": {
            "node": "./dist/cjs/transformer/index.js",
            "default": "./dist/esm5/transformer/index.js"
        },
        "./interface": {
            "node": "./dist/cjs/interface/index.js",
            "default": "./dist/esm5/interface/index.js"
        },
        "./compiler": {
            "node": "./dist/cjs/compiler/index.js",
            "default": "./dist/esm5/compiler/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adekau/semble-ts.git"
    },
    "keywords": [
        "mock",
        "data",
        "generation"
    ],
    "author": "Alex Dekau",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/adekau/semble-ts/issues"
    },
    "homepage": "https://github.com/adekau/semble-ts#readme",
    "dependencies": {
        "ts-toolbelt": "^9.6.0",
        "tslib": "^2.5.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@knodes/typedoc-plugin-pages": "^0.23.1",
        "@ts-morph/bootstrap": "^0.3.0",
        "@types/glob": "^8.1.0",
        "@types/jasmine": "^4.3.1",
        "@types/uuid": "^9.0.1",
        "fs-extra": "^11.1.0",
        "glob": "^8.1.0",
        "jasmine-core": "^4.6.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "^3.1.1",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "karma-typescript": "^5.5.3",
        "prettier": "2.3.2",
        "shx": "^0.3.4",
        "ts-transformer-testing-library": "^1.0.0-alpha.7",
        "typedoc": "^0.23.27",
        "typescript": "^5.0.2"
    }
}
