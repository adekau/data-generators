{
    "name": "data-generators",
    "version": "0.1.0",
    "description": "Mock unit test data generation",
    "main": "dist/cjs/index.js",
    "module": "dist/esm5/index.js",
    "es2015": "./dist/esm/index.js",
    "esnext": "./dist/esm5/index.js",
    "types": "./dist/types/index.d.ts",
    "typesVersions": {
        ">=5.0": {
            "*": [
                "dist/types/*"
            ]
        }
    },
    "sideEffects": false,
    "scripts": {
        "test": "rush test",
        "prettier": "prettier --write .",
        "clean:docs": "shx rm -rf ./docs",
        "build:clean": "shx rm -rf ./dist",
        "build:all": "npm run build:clean && echo \"compiling...\" && tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.types.json && echo done",
        "build": "npm run prettier && npm run build:all && node ./scripts/alias",
        "build:docs": "npm run clean:docs && typedoc",
        "fix-typedoc-pages": "shx cp ./node_modules/typedoc-default-themes/bin/default/partials/type.hbs ./node_modules/typedoc-plugin-pages/dist/theme/v2/partials/type.hbs"
    },
    "files": [
        "dist/cjs/**/*",
        "dist/esm/**/!(*.tsbuildinfo)",
        "dist/esm5/**/*",
        "dist/types/**/*"
    ],
    "exports": {
        ".": {
            "node": "./dist/cjs/index.js",
            "default": "./dist/esm5/index.js",
            "types": "./dist/types/index.d.ts"
        },
        "./core": {
            "node": "./dist/cjs/core/index.js",
            "default": "./dist/esm5/core/index.js",
            "types": "./dist/types/core/index.d.ts"
        },
        "./compiler": {
            "node": "./dist/cjs/compiler/index.js",
            "default": "./dist/esm5/compiler/index.js",
            "types": "./dist/types/compiler/index.d.ts"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adekau/data-generators.git"
    },
    "keywords": [
        "mock",
        "data",
        "generation"
    ],
    "author": "Alex Dekau",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/adekau/data-generators/issues"
    },
    "homepage": "https://github.com/adekau/data-generators#readme",
    "dependencies": {
        "ts-toolbelt": "^9.6.0",
        "tslib": "^2.5.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@knodes/typedoc-plugin-pages": "^0.23.1",
        "@types/glob": "^8.1.0",
        "@types/jasmine": "^4.3.1",
        "@types/uuid": "^9.0.1",
        "fs-extra": "^11.1.0",
        "glob": "^8.1.0",
        "jasmine-core": "^4.6.0",
        "karma": "^6.4.1",
        "karma-chrome-launcher": "^3.1.1",
        "karma-jasmine": "^5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "karma-typescript": "^5.5.3",
        "prettier": "2.3.2",
        "shx": "^0.3.4",
        "typedoc": "^0.23.27",
        "typescript": "^5.0.2"
    }
}
