{
    "name": "@nwps/data-generators",
    "version": "1.0.0-rc.4",
    "description": "Mock unit test data generation",
    "main": "dist/cjs/index.js",
    "module": "dist/esm5/index.js",
    "es2015": "./dist/esm/index.js",
    "esnext": "./dist/esm5/index.js",
    "types": "index.d.ts",
    "typesVersions": {
        ">=4.1": {
            "*": [
                "dist/types/*"
            ]
        }
    },
    "sideEffects": false,
    "scripts": {
        "test": "karma start",
        "prettier": "prettier --write .",
        "clean:docs": "shx rm -rf ./docs",
        "build:clean": "shx rm -rf ./dist",
        "build:all": "npm run build:clean && tsc -b ./src/tsconfig.cjs.json ./src/tsconfig.esm.json ./src/tsconfig.esm5.json ./src/tsconfig.types.json",
        "build": "npm run prettier && npm run build:all && node ./scripts/alias",
        "build:docs": "npm run fix-typedoc-pages && npm run clean:docs && typedoc",
        "fix-typedoc-pages": "shx cp ./node_modules/typedoc-default-themes/bin/default/partials/type.hbs ./node_modules/typedoc-plugin-pages/dist/theme/v2/partials/type.hbs"
    },
    "files": [
        "dist/cjs/**/*",
        "dist/esm/**/!(*.tsbuildinfo)",
        "dist/esm5/**/*",
        "dist/types/**/*",
        "library",
        "interface",
        "transformer",
        "tsconfig.json"
    ],
    "exports": {
        ".": {
            "node": "./dist/cjs/index.js",
            "default": "./dist/esm5/index.js"
        },
        "./library": {
            "node": "./dist/cjs/library/index.js",
            "default": "./dist/esm5/library/index.js"
        },
        "./transformer": {
            "node": "./dist/cjs/transformer/index.js",
            "default": "./dist/esm5/transformer/index.js"
        },
        "./interface": {
            "node": "./dist/cjs/interface/index.js",
            "default": "./dist/esm5/interface/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tyler-technologies/data-generators.git"
    },
    "keywords": [
        "mock",
        "data",
        "generation"
    ],
    "author": "alex.dekau@tylertech.com",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/tyler-technologies/data-generators/issues"
    },
    "homepage": "https://github.com/tyler-technologies/data-generators#readme",
    "dependencies": {
        "ts-toolbelt": "^9.6.0",
        "tslib": "^2.3.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/jasmine": "^3.8.2",
        "@types/uuid": "^8.3.1",
        "fs-extra": "^10.0.0",
        "jasmine-core": "^3.8.0",
        "karma": "^6.3.4",
        "karma-chrome-launcher": "^3.1.0",
        "karma-jasmine": "^4.0.1",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-typescript": "^5.5.1",
        "prettier": "2.3.2",
        "shx": "^0.3.3",
        "typedoc": "^0.21.5",
        "typedoc-plugin-pages": "^1.1.0",
        "typescript": "^4.3.5"
    }
}
